build-win:
    cargo build --release
    mv ./target/release/mancala.exe ./target/release/mancala-x86_64-pc-windows-msvc.exe

build-mac:
    cargo build --release
    mv ./target/release/mancala ./target/release/mancala-x86_64-apple-darwin

build-linux:
    cargo build --release
    mv ./target/release/mancala ./target/release/mancala-x86_64-unknown-linux-gnu